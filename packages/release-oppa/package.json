{
  "name": "release-oppa",
  "version": "0.2.0",
  "description": "automated semantic versioning and publishing",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "oppa": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "pnpm build",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "release-oppa"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@release-oppa/plugin": "workspace:*",
    "@release-oppa/plugin-git": "workspace:*",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.2",
    "yaml": "^2.8.2"
  }
}
